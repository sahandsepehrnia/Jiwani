<h1> Dealers </h1>
<% @member_list.each do |member| %>
	<div class="row">
    <div class="col-md-12" align="middle">
      <h3><a href="/members/<%= member.id %>/show"><%= member.first_name %> <%= member.last_name %></a></h3>
      <p class="lead"><%= member.company%></p>
      <p><%= member.location %></p>
      <p>- -</p>
    </div>
  </div>
  <% end %>
